{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "display": {
    "bar": {
      "width": 10                     // Width of percentage bars
    },
    "percent": {
      "type": 3,                       // 1=number, 2=bar, 3=both, 9=colored number
       "color": {
         "green": "green",
         "yellow": "light_yellow",
         "red": "light_red"
      }
    },
    "key": {
      "type": "icon",                  // string, icon, both, or none
      "width": 5                       // Aligns keys to this width
    },
    "separator": " â†ª",                 // Separator between keys and values
    "color": {
      "separator": "light_red",
      "title": "white",
      "keys": "white"
    }
  },
  "modules": [
    "title",
    {
      "key": " ",
      "type": "datetime",
      "format": "{month-name-short} {day-pretty}, {year}: {hour-pretty}:{minute-pretty}:{second-pretty} {timezone-name}",
      "outputColor": "bright_red"
    },
    {
      "key": " ",
      "type": "custom",
      "format": "----------------------------"
    },
    "loadavg",
    "processes",
    "break",
    "os",
    {
      "type": "kernel",
      "format": "{2}"
    },
    "uptime",
    "packages",
    "break",
    {
      "type": "memory",
      "format": "{4}{3}: {1} / {2}"
    },
    {
      "type": "swap",
      "format": "{4}{3}: {1} / {2}"
    },
    {
      "type": "disk",
      "format": "{13}{3}: {10} - {1} / {2} - {9}"
    },
    "break",
    {
      "type": "wifi",
      "format": "{1} - {2}"
    },
    {
      "type": "wifi",
      "format": "{11}{7}: {4} - {13}GHz Ch:{12} - {10} - Rx:{8} Tx:{9}"
    },
    {
      "type": "localip",
      "key": "Local IP",
      "showIpv6": true,
      "showMac": true,
      "showSpeed": true,
      "format": "{4} ({7}) - IPv4[{1}] IPv6[{2}] MAC[{3}]"
    },
    {
      "type": "publicip",
      // "showAllIps": true,
      "timeout": 1000
    },
    {
      "type": "dns",
      "showType": "both"
      // "unique": true
    },
    "netio"
  ]
}
